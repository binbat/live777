{
  "name": "webui",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@10.20.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm run dev:liveion",
    "dev:liveion": "pnpm --filter liveion dev",
    "dev:liveman": "pnpm --filter liveman dev",
    "dev:livecam": "pnpm --filter livecam dev",
    "build": "pnpm -r build",
    "build:liveion": "pnpm --filter liveion build",
    "build:liveman": "pnpm --filter liveman build",
    "build:livecam": "pnpm --filter livecam build",
    "preview": "pnpm run preview:liveion",
    "preview:liveion": "pnpm --filter liveion preview",
    "preview:liveman": "pnpm --filter liveman preview",
    "preview:livecam": "pnpm --filter livecam preview",
    "check": "biome check",
    "lint": "eslint && tsc --noEmit",
    "e2e:cluster": "vitest",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@binbat/whip-whep": "1.1.1-sdp-trickle-throw",
    "@heroicons/react": "^2.2.0",
    "@nuintun/qrcode": "^5.0.2",
    "dashjs": "^5.0.3",
    "lucide-react": "^0.552.0",
    "preact": "^10.27.2",
    "preact-router": "^4.1.2",
    "react-daisyui": "^5.0.5",
    "typescript-event-target": "^1.1.1",
    "wretch": "^3.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.3",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.56.1",
    "@preact/preset-vite": "^2.10.2",
    "@stylistic/eslint-plugin-js": "^4.4.1",
    "@types/node": "^24.10.0",
    "daisyui": "^4.12.24",
    "eslint": "^9.39.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.0",
    "vite-plugin-solid": "^2.11.10",
    "vitepress": "^1.6.4",
    "vitest": "^4.0.7"
  }
}
